{
    "name": "dimifft/ami",
    "type": "library",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/DimiFFT/PHPPDU.git"
        }
    ],
    "description": "Provide asterisk ami to laravel",
    "keywords": [
        "Enniel",
        "Ami",
        "Asterisk"
    ],
    "homepage": "https://github.com/Enniel/Ami",
    "license": "MIT",
    "authors": [
        {
            "name": "Enniel"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-mbstring": "*",
        "illuminate/support": "*",
        "illuminate/console": "*",
        "illuminate/events": "*",
        "illuminate/contracts": "*",
        "react/dns": "~0.4.3",
        "react/stream": "~0.4.6",
        "react/event-loop": "~0.4.2",
        "react/socket-client": "~0.4.6",
        "clue/ami-react": "~0.3.1",
        "dimifft/phppdu": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.0.0",
        "phpunit/phpunit": "~4.5|~5.0",
        "illuminate/config": "~5.1",
        "illuminate/container": "~5.1"
    },
    "autoload": {
        "psr-4": {
            "Enniel\\Ami\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Enniel\\Ami\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "@phpunit",
            "@phpcs"
        ],
        "phpunit": "php vendor/bin/phpunit",
        "phpcs": "php vendor/bin/php-cs-fixer --diff --dry-run -v fix"
    },
    "bin": [
        "bin/ami"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
